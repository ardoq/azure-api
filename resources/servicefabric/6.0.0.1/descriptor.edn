{:scheme "http", :host "azure.local:19080", :info {:description "Service Fabric REST Client APIs allows management of Service Fabric clusters, applications and services.", :title "Service Fabric Client APIs", :version "6.0.0.1"}, :ops {:RestartDeployedCodePackage {:path "/Nodes/{nodeName}/$/GetApplications/{applicationId}/$/GetCodePackages/$/Restart", :description "Restarts a code package deployed on a Service Fabric node in a cluster. This aborts the code package process, which will restart all the user service replicas hosted in that process.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "NodeNameRequiredPathParam"} #:reference{:parameters "ApplicationIdRequiredPathParam"} #:reference{:parameters "RestartDeployedCodePackageRequiredBodyParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :post}, :GetProvisionedFabricConfigVersionInfoList {:path "/$/GetProvisionedConfigVersions", :description "Gets a list of information about fabric config versions that are provisioned in the cluster. The parameter ConfigVersion can be used to optionally filter the output to only that particular version.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "ConfigVersionOptionalQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :GetDeployedApplicationInfo {:path "/Nodes/{nodeName}/$/GetApplications/{applicationId}", :description "Gets the information about an application deployed on a Service Fabric node.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "NodeNameRequiredPathParam"} #:reference{:parameters "ApplicationIdRequiredPathParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :ReportApplicationHealth {:path "/Applications/{applicationId}/$/ReportHealth", :description "Reports health state of the specified Service Fabric application. The report must contain the information about the source of the health report and property on which it is reported.\nThe report is sent to a Service Fabric gateway Application, which forwards to the health store.\nThe report may be accepted by the gateway, but rejected by the health store after extra validation.\nFor example, the health store may reject the report because of an invalid parameter, like a stale sequence number.\nTo see whether the report was applied in the health store, get application health and check that the report appears in the HealthEvents section.\n", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "ApplicationIdRequiredPathParam"} #:reference{:parameters "HealthInformationRequiredBodyParam"} #:reference{:parameters "ImmediateOptionalQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :post}, :GetApplicationManifest {:path "/ApplicationTypes/{applicationTypeName}/$/GetApplicationManifest", :description "Gets the manifest describing an application type. The response contains the application manifest XML as a string.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "ApplicationTypeNameRequiredPathParam"} #:reference{:parameters "ApplicationTypeVersionRequiredQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :StartDataLoss {:path "/Faults/Services/{serviceId}/$/GetPartitions/{partitionId}/$/StartDataLoss", :description "This API will induce data loss for the specified partition. It will trigger a call to the OnDataLoss API of the partition.\nActual data loss will depend on the specified DataLossMode\nPartialDataLoss - Only a quorum of replicas are removed and OnDataLoss is triggered for the partition but actual data loss depends on the presence of in-flight replication.\nFullDataLoss - All replicas are removed hence all data is lost and OnDataLoss is triggered.\n\nThis API should only be called with a stateful service as the target.\n\nCalling this API with a system service as the target is not advised.\n\nNote:  Once this API has been called, it cannot be reversed. Calling CancelOperation will only stop execution and clean up internal system state.\nIt will not restore data if the command has progressed far enough to cause data loss.\n\nCall the GetDataLossProgress API with the same OperationId to return information on the operation started with this API.\n", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "ServiceIdRequiredPathParam"} #:reference{:parameters "PartitionIdRequiredPathParam"} #:reference{:parameters "FaultOperationIdRequiredQueryParam"} #:reference{:parameters "DataLossModeRequiredQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :post}, :DeleteName {:path "/Names/{nameId}", :description "Deletes the specified Service Fabric name. A name must be created before it can be deleted. Deleting a name with child properties will fail.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "NameIdRequiredPathParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :delete}, :StartComposeDeploymentUpgrade {:path "/ComposeDeployments/{deploymentName}/$/Upgrade", :description "Validates the supplied upgrade parameters and starts upgrading the deployment if the parameters are valid.", :parameters [#:reference{:parameters "ApiVersion_6-0-Preview_RequiredQueryParam"} #:reference{:parameters "DeploymentNameRequiredPathParam"} #:reference{:parameters "ComposeDeploymentUpgradeDescriptionRequiredBodyParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :post}, :GetServiceManifest {:path "/ApplicationTypes/{applicationTypeName}/$/GetServiceManifest", :description "Gets the manifest describing a service type. The response contains the service manifest XML as a string.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "ApplicationTypeNameRequiredPathParam"} #:reference{:parameters "ApplicationTypeVersionRequiredQueryParam"} #:reference{:parameters "ServiceManifestNameRequiredQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :RemoveComposeDeployment {:path "/ComposeDeployments/{deploymentName}/$/Delete", :description "Deletes an existing Service Fabric compose deployment.", :parameters [#:reference{:parameters "ApiVersion_6-0-Preview_RequiredQueryParam"} #:reference{:parameters "DeploymentNameRequiredPathParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :post}, :GetComposeDeploymentUpgradeProgress {:path "/ComposeDeployments/{deploymentName}/$/GetUpgradeProgress", :description "Returns the information about the state of the compose deployment upgrade along with details to aid debugging application health issues.", :parameters [#:reference{:parameters "ApiVersion_6-0-Preview_RequiredQueryParam"} #:reference{:parameters "DeploymentNameRequiredPathParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :StopChaos {:path "/Tools/Chaos/$/Stop", :description "Stops Chaos from scheduling further faults; but, the in-flight faults are not affected.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :post}, :ResolveService {:path "/Services/{serviceId}/$/ResolvePartition", :description "Resolve a Service Fabric service partition, to get the endpoints of the service replicas.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "ServiceIdRequiredPathParam"} #:reference{:parameters "PartitionKeyTypeOptionalQueryParam"} #:reference{:parameters "PartitionKeyValueOptionalQueryParam"} #:reference{:parameters "PreviousRspVersionOptionalQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :CreateComposeDeployment {:path "/ComposeDeployments/$/Create", :description "Creates a Service Fabric compose deployment.", :parameters [#:reference{:parameters "ApiVersion_6-0-Preview_RequiredQueryParam"} #:reference{:parameters "CreateComposeDeploymentRequiredBodyParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :put}, :InvokeInfrastructureQuery {:path "/$/InvokeInfrastructureQuery", :description "For clusters that have one or more instances of the Infrastructure Service configured,\nthis API provides a way to send infrastructure-specific queries to a particular\ninstance of the Infrastructure Service.\n\nAvailable commands and their corresponding response formats vary depending upon\nthe infrastructure on which the cluster is running.\n\nThis API supports the Service Fabric platform; it is not meant to be used directly from your code.\n", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "InfrastructureCommandRequiredQueryParam"} #:reference{:parameters "InfrastructureServiceIdOptionalQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :CancelOperation {:path "/Faults/$/Cancel", :description "The following is a list of APIs that start fault operations that may be cancelled using CancelOperation -\n- StartDataLoss\n- StartQuorumLoss\n- StartPartitionRestart\n- StartNodeTransition\n\nIf force is false, then the specified user-induced operation will be gracefully stopped and cleaned up.  If force is true, the command will be aborted, and some internal state\nmay be left behind.  Specifying force as true should be used with care.  Calling this API with force set to true is not allowed until this API has already\nbeen called on the same test command with force set to false first, or unless the test command already has an OperationState of OperationState.RollingBack.\nClarification: OperationState.RollingBack means that the system will/is be cleaning up internal system state caused by executing the command.  It will not restore data if the\ntest command was to cause data loss.  For example, if you call StartDataLoss then call this API, the system will only clean up internal state from running the command.\nIt will not restore the target partition's data, if the command progressed far enough to cause data loss.\n\nImportant note:  if this API is invoked with force==true, internal state may be left behind.\n", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "FaultOperationIdRequiredQueryParam"} #:reference{:parameters "ForceRequiredQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :post}, :GetNodeInfo {:path "/Nodes/{nodeName}", :description "Gets the information about a specific node in the Service Fabric Cluster.The respons include the name, status, id, health, uptime and other details about the node.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "NodeNameRequiredPathParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :GetServiceHealth {:path "/Services/{serviceId}/$/GetHealth", :description "Gets the health information of the specified service.\nUse EventsHealthStateFilter to filter the collection of health events reported on the service based on the health state.\nUse PartitionsHealthStateFilter to filter the collection of partitions returned.\nIf you specify a service that does not exist in the health store, this cmdlet returns an error.\n", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "ServiceIdRequiredPathParam"} #:reference{:parameters "EventsHealthStateFilterOptionalQueryParam"} #:reference{:parameters "PartitionsHealthStateFilterOptionalQueryParam"} #:reference{:parameters "ExcludeHealthStatisticsOptionalQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :GetProvisionedFabricCodeVersionInfoList {:path "/$/GetProvisionedCodeVersions", :description "Gets a list of information about fabric code versions that are provisioned in the cluster. The parameter CodeVersion can be used to optionally filter the output to only that particular version.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "CodeVersionOptionalQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :UpdateRepairTaskHealthPolicy {:path "/$/UpdateRepairTaskHealthPolicy", :description "This API supports the Service Fabric platform; it is not meant to be used directly from your code.\n", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "RepairTaskUpdateHealthPolicyDescriptionRequiredBodyParam"}], :verb :post}, :GetReplicaInfoList {:path "/Partitions/{partitionId}/$/GetReplicas", :description "The GetReplicas endpoint returns information about the replicas of the specified partition. The respons include the id, role, status, health, node name, uptime, and other details about the replica.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "PartitionIdRequiredPathParam"} #:reference{:parameters "ContinuationTokenOptionalQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :GetQuorumLossProgress {:path "/Faults/Services/{serviceId}/$/GetPartitions/{partitionId}/$/GetQuorumLossProgress", :description "Gets the progress of a quorum loss operation started with StartQuorumLoss, using the provided OperationId.\n", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "ServiceIdRequiredPathParam"} #:reference{:parameters "PartitionIdRequiredPathParam"} #:reference{:parameters "FaultOperationIdRequiredQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :GetDeployedServiceReplicaDetailInfo {:path "/Nodes/{nodeName}/$/GetPartitions/{partitionId}/$/GetReplicas/{replicaId}/$/GetDetail", :description "Gets the details of the replica deployed on a Service Fabric node. The information include service kind, service name, current service operation, current service operation start date time, partition id, replica/instance id, reported load and other information.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "NodeNameRequiredPathParam"} #:reference{:parameters "PartitionIdRequiredPathParam"} #:reference{:parameters "ReplicaIdRequiredPathParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :GetApplicationInfoList {:path "/Applications", :description "Gets the information about the applications that were created or in the process of being created in the Service Fabric cluster and match filters specified as the parameter. The response includes the name, type, status, parameters and other details about the application. If the applications do not fit in a page, one page of results is returned as well as a continuation token which can be used to get the next page. Filters ApplicationTypeName and ApplicationDefinitionKindFilter cannot be specified at the same time.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "ApplicationDefinitionKindFilterOptionalQueryParam"} #:reference{:parameters "ApplicationTypeNameOptionalQueryParam"} #:reference{:parameters "ExcludeApplicationParametersOptionalQueryParam"} #:reference{:parameters "ContinuationTokenOptionalQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :GetApplicationLoadInfo {:path "/Applications/{applicationId}/$/GetLoadInformation", :description "Returns the load information about the application that was created or in the process of being created in the Service Fabric cluster and whose name matches the one specified as the parameter. The response includes the name, minimum nodes, maximum nodes, the number of nodes the app is occupying currently, and application load metric information about the application.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "ApplicationIdRequiredPathParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :UnprovisionCluster {:path "/$/Unprovision", :description "Unprovision the code or configuration packages of a Service Fabric cluster.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "UnprovisionFabricDescriptionRequiredBodyParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :post}, :PutProperty {:path "/Names/{nameId}/$/GetProperty", :description "Creates or updates the specified Service Fabric property under a given name.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "NameIdRequiredPathParam"} #:reference{:parameters "PropertyDescriptionRequiredBodyParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :put}, :GetServiceDescription {:path "/Services/{serviceId}/$/GetDescription", :description "Gets the description of an existing Service Fabric service. A service must be created before its description can be obtained.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "ServiceIdRequiredPathParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :GetApplicationHealth {:path "/Applications/{applicationId}/$/GetHealth", :description "Returns the heath state of the service fabric application. The response reports either Ok, Error or Warning health state. If the entity is not found in the helath store, it will return Error.", :parameters [#:reference{:parameters "ApplicationIdRequiredPathParam"} #:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "EventsHealthStateFilterOptionalQueryParam"} #:reference{:parameters "DeployedApplicationsHealthStateFilterOptionalQueryParam"} #:reference{:parameters "ServicesHealthStateFilterOptionalQueryParam"} #:reference{:parameters "ExcludeHealthStatisticsOptionalQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :GetPartitionInfo {:path "/Partitions/{partitionId}", :description "The Partitions endpoint returns information about the specified partition. The response include the partition id, partitioning scheme information, keys supported by the partition, status, health and other details about the partition.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "PartitionIdRequiredPathParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :ProvisionApplicationType {:path "/ApplicationTypes/$/Provision", :description "Provisions or registers a Service Fabric application type with the cluster. This is required before any new applications can be instantiated.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "ApplicationTypeImageStorePathRequiredBodyParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :post}, :RecoverServicePartitions {:path "/Services/$/{serviceId}/$/GetPartitions/$/Recover", :description "Indicates to the Service Fabric cluster that it should attempt to recover the specified service which is currently stuck in quorum loss. This operation should only be performed if it is known that the replicas that are down cannot be recovered. Incorrect use of this API can cause potential data loss.", :parameters [#:reference{:parameters "ServiceIdRequiredPathParam"} #:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :post}, :GetRepairTaskList {:path "/$/GetRepairTaskList", :description "This API supports the Service Fabric platform; it is not meant to be used directly from your code.\n", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "RepairTaskIdFilterOptionalQueryParam"} #:reference{:parameters "RepairTaskStateFilterOptionalQueryParam"} #:reference{:parameters "RepairTaskExecutorFilterOptionalQueryParam"}], :verb :get}, :DeleteRepairTask {:path "/$/DeleteRepairTask", :description "This API supports the Service Fabric platform; it is not meant to be used directly from your code.\n", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "RepairTaskDeleteDescriptionRequiredBodyParam"}], :verb :post}, :GetDeployedServiceTypeInfoList {:path "/Nodes/{nodeName}/$/GetApplications/{applicationId}/$/GetServiceTypes", :description "Gets the list containing the information about service types from the applications deployed on a node in a Service Fabric cluster. The response includes the name of the service type, its registration status, the code package that registered it and activation id of the service package.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "NodeNameRequiredPathParam"} #:reference{:parameters "ApplicationIdRequiredPathParam"} #:reference{:parameters "DeployedServiceType_ServiceManifestNameOptionalQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :GetClusterManifest {:path "/$/GetClusterManifest", :description "Get the Service Fabric cluster manifest. The cluster manifest contains properties of the cluster that include different node types on the cluster,\nsecurity configurations, fault and upgrade domain topologies etc.\n\nThese properties are specified as part of the ClusterConfig.JSON file while deploying a stand alone cluster. However, most of the information in the cluster manifest\nis generated internally by service fabric during cluster deployment in other deployment scenarios (for e.g when using azuer portal).\n\nThe contents of the cluster manifest are for informational purposes only and users are not expected to take a dependency on the format of the file contents or its interpretation.\n", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :GetApplicationTypeInfoListByName {:path "/ApplicationTypes/{applicationTypeName}", :description "Returns the information about the application types that are provisioned or in the process of being provisioned in the Service Fabric cluster. These results are of application types whose name match exactly the one specified as the parameter, and which comply with the given query parameters. All versions of the application type matching the application type name are returned, with each version returned as one application type. The response includes the name, version, status and other details about the application type. This is a paged query, meaning that if not all of the application types fit in a page, one page of results is returned as well as a continuation token which can be used to get the next page. For example, if there are 10 application types but a page only fits the first 3 application types, or if max results is set to 3, then 3 is returned. To access the rest of the results, retrieve subsequent pages by using the returned continuation token in the next query. An empty continuation token is returned if there are no subsequent pages.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "ApplicationTypeNameRequiredPathParam"} #:reference{:parameters "ApplicationTypeVersionOptionalQueryParam"} #:reference{:parameters "ExcludeApplicationParametersOptionalQueryParam"} #:reference{:parameters "ContinuationTokenOptionalQueryParam"} #:reference{:parameters "MaxResultsOptionalQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :GetDataLossProgress {:path "/Faults/Services/{serviceId}/$/GetPartitions/{partitionId}/$/GetDataLossProgress", :description "Gets the progress of a data loss operation started with StartDataLoss, using the OperationId.\n", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "ServiceIdRequiredPathParam"} #:reference{:parameters "PartitionIdRequiredPathParam"} #:reference{:parameters "FaultOperationIdRequiredQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :GetComposeDeploymentStatusList {:path "/ComposeDeployments", :description "Gets the status about the compose deployments that were created or in the process of being created in the Service Fabric cluster. The response includes the name, status and other details about the compose deployments. If the list of deployments do not fit in a page, one page of results is returned as well as a continuation token which can be used to get the next page.", :parameters [#:reference{:parameters "ApiVersion_6-0-Preview_RequiredQueryParam"} #:reference{:parameters "ContinuationTokenOptionalQueryParam"} #:reference{:parameters "MaxResultsOptionalQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :GetImageStoreContent {:path "/ImageStore/{contentPath}", :description "Returns the information about the image store content at the specified contentPath relative to the root of the image store.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "ImageStoreContentPathRequiredPathParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :GetDeployedApplicationInfoList {:path "/Nodes/{nodeName}/$/GetApplications", :description "Gets the list of applications deployed on a Service Fabric node.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "NodeNameRequiredPathParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :GetPartitionHealthUsingPolicy {:path "/Partitions/{partitionId}/$/GetHealth", :description "Gets the health information of the specified partition.\nIf the application health policy is specified, the health evaluation uses it to get the aggregated health state.\nIf the policy is not specified, the health evaluation uses the application health policy defined in the application manifest, or the default health policy, if no policy is defined in the manifest.\nUse EventsHealthStateFilter to filter the collection of health events reported on the partition based on the health state.\nUse ReplicasHealthStateFilter to filter the collection of ReplicaHealthState objects on the partition. Use ApplicationHealthPolicy in the POST body to override the health policies used to evaluate the health.\nIf you specify a partition that does not exist in the health store, this cmdlet returns an error.\n", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "PartitionIdRequiredPathParam"} #:reference{:parameters "EventsHealthStateFilterOptionalQueryParam"} #:reference{:parameters "ReplicasHealthStateFilerOptionalQueryParam"} #:reference{:parameters "ApplicationHealthPolicyOptionalBodyParam"} #:reference{:parameters "ExcludeHealthStatisticsOptionalQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :post}, :UpdateService {:path "/Services/{serviceId}/$/Update", :description "Updates the specified service using the given update description.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "ServiceIdRequiredPathParam"} #:reference{:parameters "ServiceUpdateDescriptionRequiredBodyParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :post}, :ReportPartitionHealth {:path "/Partitions/{partitionId}/$/ReportHealth", :description "Reports health state of the specified Service Fabric partition. The report must contain the information about the source of the health report and property on which it is reported.\nThe report is sent to a Service Fabric gateway Partition, which forwards to the health store.\nThe report may be accepted by the gateway, but rejected by the health store after extra validation.\nFor example, the health store may reject the report because of an invalid parameter, like a stale sequence number.\nTo see whether the report was applied in the health store, run GetPartitionHealth and check that the report appears in the HealthEvents section.\n", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "PartitionIdRequiredPathParam"} #:reference{:parameters "HealthInformationRequiredBodyParam"} #:reference{:parameters "ImmediateOptionalQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :post}, :GetComposeDeploymentStatus {:path "/ComposeDeployments/{deploymentName}", :description "Returns the status of the compose deployment that was created or in the process of being created in the Service Fabric cluster and whose name matches the one specified as the parameter. The response includes the name, status and other details about the deployment.", :parameters [#:reference{:parameters "ApiVersion_6-0-Preview_RequiredQueryParam"} #:reference{:parameters "DeploymentNameRequiredPathParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :GetDeployedServiceTypeInfoByName {:path "/Nodes/{nodeName}/$/GetApplications/{applicationId}/$/GetServiceTypes/{serviceTypeName}", :description "Gets the list containing the information about a specific service type from the applications deployed on a node in a Service Fabric cluster. The response includes the name of the service type, its registration status, the code package that registered it and activation id of the service package. Each entry represents one activation of a service type, differentiated by the activation id.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "NodeNameRequiredPathParam"} #:reference{:parameters "ApplicationIdRequiredPathParam"} #:reference{:parameters "ServiceTypeNameRequiredPathParam"} #:reference{:parameters "DeployedServiceType_ServiceManifestNameOptionalQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :GetApplicationNameInfo {:path "/Services/{serviceId}/$/GetApplicationName", :description "The GetApplicationName endpoint returns the name of the application for the specified service.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "ServiceIdRequiredPathParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :GetNameExistsInfo {:path "/Names/{nameId}", :description "Returns whether the specified Service Fabric name exists.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "NameIdRequiredPathParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :CreateApplication {:path "/Applications/$/Create", :description "Creates a Service Fabric application using the specified description.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "ApplicationDescriptionRequiredBodyParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :post}, :UploadFile {:path "/ImageStore/{contentPath}", :description "Uploads contents of the file to the image store. Use this API if the file is small enough to upload again if the connection fails. The file's data needs to be added to the request body. The contents will be uploaded to the specified path. Image store service uses a mark file to indicate the availability of the folder. The mark file is an empty file named \"_.dir\". The mark file is generated by the image store service when all files in a folder are uploaded. When using File-by-File approach to upload application package in REST, the image store service isn't aware of the file hierarchy of the application package; you need to create a mark file per folder and upload it last, to let the image store service know that the folder is complete.\n", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "ImageStoreContentPathRequiredPathParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :put}, :GetDeployedServicePackageHealth {:path "/Nodes/{nodeName}/$/GetApplications/{applicationId}/$/GetServicePackages/{servicePackageName}/$/GetHealth", :description "Gets the information about health of service package for a specific application deployed on a Service Fabric node. Use EventsHealthStateFilter to optionally filter for the collection of HealthEvent objects reported on the deployed service package based on health state.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "NodeNameRequiredPathParam"} #:reference{:parameters "ApplicationIdRequiredPathParam"} #:reference{:parameters "ServicePackageNameRequiredPathParam"} #:reference{:parameters "EventsHealthStateFilterOptionalQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :CreateService {:path "/Applications/{applicationId}/$/GetServices/$/Create", :description "Creates the specified service.", :parameters [#:reference{:parameters "ApplicationIdRequiredPathParam"} #:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "ServiceDescriptionRequiredBodyParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :post}, :ForceApproveRepairTask {:path "/$/ForceApproveRepairTask", :description "This API supports the Service Fabric platform; it is not meant to be used directly from your code.\n", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "RepairTaskApproveDescriptionRequiredBodyParam"}], :verb :post}, :StartPartitionRestart {:path "/Faults/Services/{serviceId}/$/GetPartitions/{partitionId}/$/StartRestart", :description "This API is useful for testing failover.\n\nIf used to target a stateless service partition, RestartPartitionMode must be AllReplicasOrInstances.\n\nCall the GetPartitionRestartProgress API using the same OperationId to get the progress.\n", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "ServiceIdRequiredPathParam"} #:reference{:parameters "PartitionIdRequiredPathParam"} #:reference{:parameters "FaultOperationIdRequiredQueryParam"} #:reference{:parameters "RestartPartitionModeRequiredQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :post}, :GetFaultOperationList {:path "/Faults/", :description "Gets the a list of user-induced fault operations filtered by provided input.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "TypeFilterRequiredQueryParam"} #:reference{:parameters "StateFilterRequiredQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :StartQuorumLoss {:path "/Faults/Services/{serviceId}/$/GetPartitions/{partitionId}/$/StartQuorumLoss", :description "Induces quorum loss for a given stateful service partition.  This API is useful for a temporary quorum loss situation on your service.\n\nCall the GetQuorumLossProgress API with the same OperationId to return information on the operation started with this API.\n\nThis can only be called on stateful persisted (HasPersistedState==true) services.  Do not use this API on stateless services or stateful in-memory only services.\n", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "ServiceIdRequiredPathParam"} #:reference{:parameters "PartitionIdRequiredPathParam"} #:reference{:parameters "FaultOperationIdRequiredQueryParam"} #:reference{:parameters "QuorumLossModeRequiredQueryParam"} #:reference{:parameters "QuorumLossDurationRequiredQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :post}, :DeployedServicePackageToNode {:path "/Nodes/{nodeName}/$/DeployServicePackage", :description "Downloads packages associated with specified service manifest to image cache on specified node.\n", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "NodeNameRequiredPathParam"} #:reference{:parameters "DeployServicePackageToNodeRequiredBodyParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :post}, :GetChaosReport {:path "/Tools/Chaos/$/Report", :description "You can either specify the ContinuationToken to get the next segment of the Chaos report or you can specify the time-range\nthrough StartTimeUtc and EndTimeUtc, but you cannot specify both the ContinuationToken and the time-range in the same call.\nWhen there are more than 100 Chaos events, the Chaos report is returned in segments where a segment contains no more than 100 Chaos events.\n", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "ContinuationTokenOptionalQueryParam"} #:reference{:parameters "StartTimeUtcOptionalQueryParam"} #:reference{:parameters "EndTimeUtcOptionalQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :EnableNode {:path "/Nodes/{nodeName}/$/Activate", :description "Activates a Service Fabric cluster node which is currently deactivated. Once activated, the node will again become a viable target for placing new replicas, and any deactivated replicas remaining on the node will be reactivated.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "NodeNameRequiredPathParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :post}, :GetApplicationInfo {:path "/Applications/{applicationId}", :description "Returns the information about the application that was created or in the process of being created in the Service Fabric cluster and whose name matches the one specified as the parameter. The response includes the name, type, status, parameters and other details about the application.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "ApplicationIdRequiredPathParam"} #:reference{:parameters "ExcludeApplicationParametersOptionalQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :GetNodeHealth {:path "/Nodes/{nodeName}/$/GetHealth", :description "Gets the health of a Service Fabric node. Use EventsHealthStateFilter to filter the collection of health events reported on the node based on the health state. If the node that you specify by name does not exist in the health store, this returns an error.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "NodeNameRequiredPathParam"} #:reference{:parameters "EventsHealthStateFilterOptionalQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :RestartNode {:path "/Nodes/{nodeName}/$/Restart", :description "Restarts a Service Fabric cluster node that is already started.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "NodeNameRequiredPathParam"} #:reference{:parameters "RestartNodeDescriptionRequiredBodyParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :post}, :GetServiceHealthUsingPolicy {:path "/Services/{serviceId}/$/GetHealth", :description "Gets the health information of the specified service.\nIf the application health policy is specified, the health evaluation uses it to get the aggregated health state.\nIf the policy is not specified, the health evaluation uses the application health policy defined in the application manifest, or the default health policy, if no policy is defined in the manifest.\nUse EventsHealthStateFilter to filter the collection of health events reported on the service based on the health state.\nUse PartitionsHealthStateFilter to filter the collection of partitions returned.\nIf you specify a service that does not exist in the health store, this cmdlet returns an error.\n", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "ServiceIdRequiredPathParam"} #:reference{:parameters "EventsHealthStateFilterOptionalQueryParam"} #:reference{:parameters "PartitionsHealthStateFilterOptionalQueryParam"} #:reference{:parameters "ApplicationHealthPolicyOptionalBodyParam"} #:reference{:parameters "ExcludeHealthStatisticsOptionalQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :post}, :UpdateRepairExecutionState {:path "/$/UpdateRepairExecutionState", :description "This API supports the Service Fabric platform; it is not meant to be used directly from your code.\n", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "RepairTaskRequiredBodyParam"}], :verb :post}, :ProvisionCluster {:path "/$/Provision", :description "Validate and provision the code or configuration packages of a Service Fabric cluster.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "ProvisionFabricDescriptionRequiredBodyParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :post}, :ResetPartitionLoad {:path "/Partitions/{partitionId}/$/ResetLoad", :description "Resets the current load of a Service Fabric partition to the default load for the service.", :parameters [#:reference{:parameters "PartitionIdRequiredPathParam"} #:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :post}, :GetClusterHealthChunkUsingPolicyAndAdvancedFilters {:path "/$/GetClusterHealthChunk", :description "Gets the health of a Service Fabric cluster using health chunks. The health evaluation is done based on the input cluster health chunk query description.\nThe query description allows users to specify health policies for evaluating the cluster and its children.\nUsers can specify very flexible filters to select which cluster entities to return. The selection can be done based on the entities health state and based on the hierarchy.\nThe query can return multi-level children of the entities based on the specified filters. For example, it can return one application with a specified name, and for this application, return\nonly services that are in Error or Warning, and all partitions and replicas for one of these services.\n", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "ClusterHealthChunkQueryDescriptionOptionalBodyParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :post}, :GetDeployedServicePackageInfoListByName {:path "/Nodes/{nodeName}/$/GetApplications/{applicationId}/$/GetServicePackages/{servicePackageName}", :description "Returns the information about the service packages deployed on a Service Fabric node for the given application. These results are of service packages whose name match exactly the service package name specified as the parameter.", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "NodeNameRequiredPathParam"} #:reference{:parameters "ApplicationIdRequiredPathParam"} #:reference{:parameters "ServicePackageNameRequiredPathParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :CancelRepairTask {:path "/$/CancelRepairTask", :description "This API supports the Service Fabric platform; it is not meant to be used directly from your code.\n", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "RepairTaskCancelDescriptionRequiredBodyParam"}], :verb :post}, :GetPartitionRestartProgress {:path "/Faults/Services/{serviceId}/$/GetPartitions/{partitionId}/$/GetRestartProgress", :description "Gets the progress of a PartitionRestart started with StartPartitionRestart using the provided OperationId.\n", :parameters [#:reference{:parameters "ApiVersionRequiredQueryParam"} #:reference{:parameters "ServiceIdRequiredPathParam"} #:reference{:parameters "PartitionIdRequiredPathParam"} #:reference{:parameters "FaultOperationIdRequiredQueryParam"} #:reference{:parameters "TimeoutOptionalQueryParam"}], :verb :get}, :GetClusterConfiguration {:path "/$/GetClusterConfiguration", :description "Get the Service Fabric standalone cluster configuration. The cluster configurati